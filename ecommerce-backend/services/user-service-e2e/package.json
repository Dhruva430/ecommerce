{
  "name": "@ecommerce-backend/user-service-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@ecommerce-backend/user-service"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "services/user-service-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@ecommerce-backend/user-service:build",
          "@ecommerce-backend/user-service:serve"
        ]
      }
    }
  }
}
